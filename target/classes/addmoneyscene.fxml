    <children>
        <MenuButton fx:id="pick_account" layoutX="31.0" layoutY="21.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="175.0" text="Выберите счет" />
        <Button fx:id="submit_btn" disable="true" layoutX="31.0" layoutY="113.0" mnemonicParsing="false" onAction="#submitClick" prefHeight="25.0" prefWidth="175.0" text="Добавить на счет" />
      <TextField fx:id="money_textfield" layoutX="31.0" layoutY="67.0" prefHeight="25.0" prefWidth="175.0" promptText="Сумма" />
    </children>
</AnchorPane>
